<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
       <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:sheet.type</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.isMobile>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isMobile</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isMobile.description</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isMobile>
                    <settings.isWireless>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isWireless</label>
                            <displayCond>FIELD:settings.isMobile:REQ:false</displayCond>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isWireless.description</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isWireless>
                    <settings.isTablet>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isTablet</label>
                            <displayCond>FIELD:settings.isMobile:REQ:false</displayCond>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isTablet.description</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isTablet>
                    <settings.isSmartTv>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isSmartTv</label>
                            <displayCond>FIELD:settings.isMobile:REQ:false</displayCond>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isSmartTv.description</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isSmartTv>
                    <settings.isPhone>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isPhone</label>
                            <displayCond>FIELD:settings.isMobile:REQ:false</displayCond>
                            <config>
                                <type>check</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.isPhone.description</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isPhone>
                </el>
            </ROOT>
        </sDEF>
        <sDimension>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:sheet.dimension</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.screenWidthMin>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.screenWidthMin</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,num,int</eval>
                            </config>
                        </TCEforms>
                    </settings.screenWidthMin>
                    <settings.screenWidthMax>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.screenWidthMax</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,num,int</eval>
                            </config>
                        </TCEforms>
                    </settings.screenWidthMax>
                    <settings.screenHeightMin>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.screenHeightMin</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,num,int</eval>
                            </config>
                        </TCEforms>
                    </settings.screenHeightMin>
                    <settings.screenHeightMax>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.screenHeightMax</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>trim,num,int</eval>
                            </config>
                        </TCEforms>
                    </settings.screenHeightMax>
                </el>
            </ROOT>
        </sDimension>
        <sProductInfo>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:sheet.productInfo</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.brandName>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.brandName</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
<!--
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
-->
                                <itemsProcFunc>Tx_Contexts_Wurfl_Backend-&gt;getBrandNames</itemsProcFunc>
                                <size>10</size>
                                <default></default>
                                <minitems>0</minitems>
                                <maxitems>100</maxitems>
                            </config>
                        </TCEforms>
                    </settings.brandName>
                    <settings.modelName>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.modelName</label>
                            <displayCond>FIELD:settings.brandName:REQ:true</displayCond>
                            <config>
                                <type>select</type>
<!--
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
-->
                                <itemsProcFunc>Tx_Contexts_Wurfl_Backend-&gt;getModelNames</itemsProcFunc>
                                <size>10</size>
                                <default></default>
                                <minitems>0</minitems>
                                <maxitems>100</maxitems>
                            </config>
                        </TCEforms>
                    </settings.modelName>
                    <settings.mobileBrowser>
                        <TCEforms>
                            <label>LLL:EXT:contexts_wurfl/Resources/Private/Language/locallang_flex.xml:settings.mobileBrowser</label>
                            <config>
                                <type>select</type>
<!--
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
-->
                                <itemsProcFunc>Tx_Contexts_Wurfl_Backend-&gt;getMobileBrowsers</itemsProcFunc>
                                <size>10</size>
                                <default></default>
                                <minitems>0</minitems>
                                <maxitems>100</maxitems>
                            </config>
                        </TCEforms>
                    </settings.mobileBrowser>
                </el>
            </ROOT>
        </sProductInfo>
    </sheets>
</T3DataStructure>
